{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngx-voyage": {
      "projectType": "library",
      "root": "projects/ngx-voyage",
      "sourceRoot": "projects/ngx-voyage/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "options": {
            "project": "projects/ngx-voyage/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-voyage/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ngx-voyage/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        }
      }
    },
    "voyage": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/voyage",
      "sourceRoot": "projects/voyage/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/voyage",
            "index": "projects/voyage/src/index.html",
            "browser": "projects/voyage/src/main.ts",
            "tsConfig": "projects/voyage/tsconfig.app.json",
            "assets": [
              {
                "input": "dist/ngx-voyage/assets",
                "glob": "*",
                "output": "assets"
              },
              {
                "glob": "**/*",
                "input": "projects/voyage/public"
              }
            ],
            "styles": [
              "projects/voyage/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2MB",
                  "maximumError": "3MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "80kB",
                  "maximumError": "100kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "voyage:build:production"
            },
            "development": {
              "buildTarget": "voyage:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "proxyConfig": "proxy.conf.json"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/voyage/tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/voyage/public"
              }
            ],
            "styles": [
              "projects/voyage/src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}